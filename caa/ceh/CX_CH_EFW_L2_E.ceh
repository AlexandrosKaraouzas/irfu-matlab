  UNITS             = "mV/m"
  FILLVAL           = -1000000000.000
  QUALITY           = E_quality__CXXX_CP_EFW_L2_E
  SIGNIFICANT_DIGITS= 6
  PARAMETER_CAVEATS = "For offsets see DER dataset for this interval. Probes: 34"
  COORDINATE_SYSTEM = "ISR2"
  FRAME_VELOCITY    = "Observatory"
