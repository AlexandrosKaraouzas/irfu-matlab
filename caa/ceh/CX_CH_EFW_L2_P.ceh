START_VARIABLE    = time_tags__CXXX_CP_EFW_L2_P
  PARAMETER_TYPE    = "Support_Data"
  VALUE_TYPE      = ISO_TIME
  DELTA_PLUS      = 0.1
  DELTA_MINUS     = 0.1
  FILLVAL         = 9999-12-31T23:59:59Z
  LABLAXIS        = "UT"
  FIELDNAM        = "Universal Time"
END_VARIABLE      = time_tags__CXXX_CP_EFW_L2_P
!
START_VARIABLE      = Spacecraft_potential__CXXX_CP_EFW_L2_P
  PARAMETER_TYPE    = "Data"
  SIZES             = 1
  VALUE_TYPE        = FLOAT
  ENTITY            = "Instrument"
  PROPERTY          = "Probe_Potential"
  FLUCTUATIONS      = "Waveform"
  CATDESC           = "Spacecraft potential"
  FIELDNAM          = "Spacecraft potential"
  SI_CONVERSION     = "1>V"
  UNITS             = "V"
  FILLVAL           = -1000000000.000
  QUALITY           = 3
  SIGNIFICANT_DIGITS= 6
  PARAMETER_CAVEATS = "*CXXX_CP_EFW_L2_P"
  LABLAXIS          = "-Sc pot"
  DEPEND_0          = time_tags__CXXX_CP_EFW_L2_P
END_VARIABLE        = Spacecraft_potential__CXXX_CP_EFW_L2_P
