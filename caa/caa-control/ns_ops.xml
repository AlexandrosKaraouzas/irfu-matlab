<!DOCTYPE table [
	<!ELEMENT table (title,description,operation+)>
		<!ELEMENT title (#PCDATA)>
		<!ELEMENT description (#PCDATA)>
		<!ELEMENT operation (desc,res*)>
			<!ATTLIST operation start NMTOKEN #REQUIRED>
			<!ATTLIST operation dt NMTOKEN #REQUIRED>
			<!ATTLIST operation sdesc ( no_tm|no_bm3|bad_tm|bad_data|no_p1|no_p2|no_p3|no_p4|no_p12|no_p34|no_p23|no_10Hz_filt|no_spin_fits|spec_bias|TBD ) #REQUIRED>
